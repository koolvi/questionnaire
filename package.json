{
  "name": "questionnaire",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "nodemailer": "^6.4.17",
    "nodemailer-smtp-transport": "^2.7.4",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "react-text-mask": "^5.4.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "front:build": "react-scripts build",
    "api:dev": "yarn babel-node src/api/index.jsx",
    "api:build": "yarn babel src/api -d api-build",
    "api:prod": "rm -r api-build && yarn api:build && node api-build/index.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/prefer-stateless-function": 0,
      "no-underscore-dangle": 0,
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "arrow-body-style": 0,
      "no-nested-ternary": 0,
      "object-curly-newline": 0,
      "import/no-unresolved": 0,
      "no-use-before-define": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "prop-types": "^15.6.0",
    "typescript": "^4.1.3"
  },
  "proxy": "http://localhost:4000"
}
